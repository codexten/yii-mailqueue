{
  "name": "codexten/yii-mailqueue",
  "type": "yii2-extension",
  "description": "Codexten Yii Mailer",
  "require": {
    "codexten/yii-core": "~2.0",
    "yiisoft/yii2-swiftmailer": "~2.0.7",
    "katzgrau/klogger": "~1.2.1"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\mailqueue\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "mailer": "src/config/mailer.php",
      "params": "src/config/params.php",
      "core": "src/config/core.php",
      "migrationNamespaces": "src/config/migrationNamespaces.php",
      "console": "src/config/console.php"
    }
  }
}
